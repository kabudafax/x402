# Monad Testnet Deployment Configuration
# Copy this file to .env and fill in your values

# Deployer private key (MUST start with 0x prefix)
# Example: PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PRIVATE_KEY=0xyour_private_key_here

# Mock USDC token address (optional - only needed if deploying example services)
# If not provided, example services will not be deployed
MOCK_USDC_ADDRESS=

# Monad Explorer API key (optional - for contract verification)
MONAD_API_KEY=
